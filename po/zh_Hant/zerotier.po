# Installation method translations
msgid "Installation & Setup Guide"
msgstr "安裝和設定指南"

msgid "ZTNCUI is not available in standard OpenWrt packages. Choose one of the following installation methods:"
msgstr "ZTNCUI 在標準 OpenWrt 軟體包中不可用。請選擇以下安裝方法之一："

msgid "Method 1: Docker (Recommended)"
msgstr "方法 1：Docker（推薦）"

msgid "Method 2: Node.js Installation"
msgstr "方法 2：Node.js 安裝"

msgid "Method 3: External Server"
msgstr "方法 3：外部伺服器"

msgid "Install Docker: opkg install dockerd docker"
msgstr "安裝 Docker：opkg install dockerd docker"

msgid "Start Docker service: /etc/init.d/dockerd start"
msgstr "啟動 Docker 服務：/etc/init.d/dockerd start"

msgid "Run ZTNCUI container: docker run -d --name ztncui --restart=unless-stopped -p 3000:3000 -v /var/lib/zerotier-one:/var/lib/zerotier-one keynetworks/ztncui"
msgstr "執行 ZTNCUI 容器：docker run -d --name ztncui --restart=unless-stopped -p 3000:3000 -v /var/lib/zerotier-one:/var/lib/zerotier-one keynetworks/ztncui"

msgid "Install Node.js: opkg install node npm"
msgstr "安裝 Node.js：opkg install node npm"

msgid "Install ZTNCUI: npm install -g ztncui"
msgstr "安裝 ZTNCUI：npm install -g ztncui"

msgid "Run ZTNCUI: ztncui"
msgstr "執行 ZTNCUI：ztncui"

msgid "Note: Requires significant storage space (100MB+)"
msgstr "注意：需要大量儲存空間（100MB+）"

msgid "Run ZTNCUI on a separate device and configure ZeroTier controller to use this OpenWrt device as backend."
msgstr "在單獨的裝置上執行 ZTNCUI，并設定 ZeroTier 控制器使用這個 OpenWrt 裝置作為後端。"

