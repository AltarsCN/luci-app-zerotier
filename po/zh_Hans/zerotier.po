# Installation method translations
msgid "Installation & Setup Guide"
msgstr "安装和设置指南"

msgid "ZTNCUI is not available in standard OpenWrt packages. Choose one of the following installation methods:"
msgstr "ZTNCUI 在标准 OpenWrt 软件包中不可用。请选择以下安装方法之一："

msgid "Method 1: Docker (Recommended)"
msgstr "方法 1：Docker（推荐）"

msgid "Method 2: Node.js Installation"
msgstr "方法 2：Node.js 安装"

msgid "Method 3: External Server"
msgstr "方法 3：外部服务器"

msgid "Install Docker: opkg install dockerd docker"
msgstr "安装 Docker：opkg install dockerd docker"

